{
  "name": "my-nest-angular-app",
  "version": "1.0.0",
  "scripts": {
    "start:frontend": "cd src/frontend && ng serve",
    "start:backend": "cd src/backend && nest start",
    "start": "npm run start:frontend\" \"npm run start:backend\"",
    "build:frontend": "cd src/frontend && ng build",
    "build:backend": "cd src/backend && nest build",
    "build": "npm run build:frontend && npm run build:backend",
    "install:all": "npm install && cd src/frontend && npm install && cd ../backend && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.0.1"
  },
  "dependencies": {
    "@angular/common": "^19.1.1",
    "@angular/core": "^19.1.1",
    "@angular/forms": "^19.1.1",
    "@angular/platform-browser": "^19.1.1",
    "@angular/platform-browser-dynamic": "^19.1.1",
    "@angular/router": "^19.1.1",
    "axios": "^1.7.9",
    "typescript": "^5.7.3"
  }
}
